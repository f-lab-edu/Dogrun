{
    "code": 200,
    "data": {
        "uid": "123456",
        "name": "Buddy",
        "breed": "Golden Retriever",
        "birth": "2018-05-15",
        "gender": "male",
        "size": "large"
    },
    "msg": "Success"
}
final class UserDefaultsRepositoryTests: XCTestCase {
    
    var sut: UserDefaultsRepository!

    override func setUpWithError() throws {
        try super.setUpWithError()
        sut = UserDefaultsRepository()
    }

    override func tearDownWithError() throws {
        sut = nil
        try super.tearDownWithError()
    }
    
    func testGetUserInfo() {
      
        // Given
        let userInfo = UserInfo.Builder()
            .setUID("12345")
            .setEmail("example@example.com")
            .setName("John Doe")
            .setBirth("1990-01-01")
            .setArea("New York")
            .build()
        
        let keys = "userInfoKey"
        userDefaultsRepository.setUserInfo(userInfo: userInfo, keys: keys)

        // When
        let retrievedUserInfo = userDefaultsRepository.getUserInfo(keys: keys)

        // Then
        XCTAssertNotNil(retrievedUserInfo, "Retrieved user info should not be nil")
        XCTAssertEqual(retrievedUserInfo?.name, "John", "Retrieved user info's name should be 'John'")
        XCTAssertEqual(retrievedUserInfo?.age, 30, "Retrieved user info's age should be 30")
     }
}
